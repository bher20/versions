<h1>Users</h1>
<table class="table table-striped">
 <thead>
  <tr>
    <th>ID</th>
    <th><%= t("users.username") %></th>
    <th><%= t("users.email") %></th>
    <th>Created at</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| -%>
   <tr>
    <td><%= user.id %></td>
     <td><%= link_to user.username, admin_user_path(user) %></td>
    <td><%= user.email %></td>
    <td><%= user.created_at %></td>
    <td>
      <%= link_to "Show", admin_user_path(user) %>
      <%= link_to "Edit", edit_admin_user_path(user) %>
      <%= link_to "Destroy", admin_user_path(user), :method => :delete, :confirm => "#{t('general.are_you_sure')}" %>
    </td>
    </tr>
    <% end -%>
   </tbody>
</table>
<br />
<%= link_to 'New', new_admin_user_path, :class => 'btn btn-primary' %>
